{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Williw Desktop",
  "version": "0.1.1",
  "identifier": "com.williw.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "Williw - Decentralized Training",
        "width": 1150,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "hiddenTitle": false,
        "titleBarStyle": "Visible"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/williw.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
